    "marray": "^1.1.6",
    "ptn": "^0.2.0",
    "quasar": "^1.7.0",
    "vue": "^2.6.11",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-i18n": "^7.3.3",
    "vue-moment": "^4.1.0",
