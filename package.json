{
  "name": "ptn-ninja",
  "version": "3.2.0",
  "description": "An editor and viewer for Portable Tak Notation",
  "productName": "PTN Ninja",
  "author": "Craig Laparo <gruppler+github@gmail.com>",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "icons": "icongenie g -i app-icon.png --quality 12 --theme-color 607d8b --skip-trim",
    "dev": "quasar dev -m spa",
    "dev:pwa": "quasar dev -m pwa",
    "deploy": "quasar build -m pwa && firebase deploy --only hosting",
    "deploy:dev": "quasar build -m pwa && firebase deploy --only hosting:ptn-ninja-dev",
    "deploy:staging": "quasar build -m pwa && firebase deploy --only hosting:ptn-ninja-staging",
    "deploy:live": "quasar build -m pwa && firebase deploy --only hosting:ptn-ninja",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "deploy:functions": "firebase deploy --only functions",
    "emulate": "firebase emulators:start --import ./db --export-on-exit"
  },
  "dependencies": {
    "@quasar/extras": "^1.11.1",
    "aggregation": "^1.2.6",
    "core-js": "^3.8.1",
    "date-fns": "^2.16.1",
    "diff-match-patch": "^1.0.4",
    "firebase": "^8.2.0",
    "firebase-functions": "^3.13.0",
    "firebase-tools": "^8.19.0",
    "lodash": "^4.17.14",
    "lz-string": "^1.4.4",
    "marray": "^1.1.6",
    "quasar": "^1.16.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.22.2",
    "vue-qriously": "^1.1.1",
    "vue-shortkey": "^3.1.7",
    "vue-smooth-reflow": "^0.1.12"
  },
  "devDependencies": {
    "@quasar/app": "^2.2.8",
    "@quasar/cli": "^1.2.2",
    "@quasar/quasar-app-extension-qmarkdown": "^1.4.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.15.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^7.2.0",
    "firebase-admin": "^9.4.2",
    "prettier": "^2.2.1",
    "webpack": "^5.10.1",
    "workbox-webpack-plugin": "^4.0.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
